<Window x:Class="wpf_project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpf_project"
        mc:Ignorable="d"
        Title="Cooker Shop" Height="600" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Image Grid.Row="0" Height="40" Margin="20,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" Width="40" Source="Resources\pngegg.png"/>
        <Button x:Name="register" Content="Регистрация" Margin="0,0,200,0" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Roboto" FontWeight="Bold" FontSize="25" Padding="10,5,10,5" Click="register_Click"/>
        <Button x:Name="autorizate" Content="Авторизация" Margin="200,0,0,0" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Roboto" FontWeight="Bold" FontSize="25" Padding="10,5,10,5" Click="autorizate_Click"/>
        <Frame Grid.Row="1" Name="fMain" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
    
</Window>
